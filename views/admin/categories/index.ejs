<%- include("../../partials/header.ejs") %>
<%- include("../../partials/navbar.ejs") %>
    <body>
        <div class="container">
            <hr>
            <h2>Categorias</h2>
            <hr>
            <table class="table table-bordered">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Título</th>
                        <th>Slug</th>
                        <th>Ações</th>
                    </tr>
                </thead>
                <tbody>
                    <% categories.forEach((categoria) =>{ %>
                        <tr>
                            <th><%= categoria.id %></th>
                            <th><%= categoria.title %></th>
                            <th><%= categoria.slug %></th>
                            <th>Ações</th>
                        </tr>
                    <% }) %>
                </tbody>
            </table>
        </div>
    <body>
<%- include("../../partials/footer.ejs") %>          